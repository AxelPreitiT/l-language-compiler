/*
    Esto es un
    comentario
    en bloque

    proces A [
        $0 = #0;
        #1 = AnD($5, 0);
        #2 = OR(true, FalSe);
    ]

    INPUT(2);
    INPUT -> OUTPUT -> A -> OUTPUT;

*/
proc A {
    aux = AND/*
    Esto es un
    comentario
    en bloque

    proces A [
        $0 = #0;
        #1 = AnD($5, 0);
        #2 = OR(true, FalSe);
    ]

    INPUT(2);
    INPUT -> OUTPUT -> A -> OUTPUT;

    */($0, $1);
    #0 = OR(aux, $2);
};

INPUT(3);
INPUT -> A -> OUTPUT;